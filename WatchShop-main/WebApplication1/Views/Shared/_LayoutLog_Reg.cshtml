                                            <ul>
                                                @{
                                                    Html.RenderAction("getCate", "Home");
                                                }
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col2">
                                        <div class="h_nav">
                                            <h4>Trends</h4>
                                            <ul>
                                                @{
                                                    Html.RenderAction("trend", "Home");
                                                }
                                            </ul>
